-- Find intersection of two sorted arrays --

precondition: array(a, b) * (b+1) = (a+n) * array(c, d) * (c+m) = (d+1);
if n<m then
	s := n
else
	s := m
fi;
x := new array[s];
i := 0;
r := 0;
l := 0;
while i<s do
	if m <= r then
		stop
	fi;
	if n <= l then
		stop
	fi;
	t := a[l];
	w := c[r];
	if t = w then
		x[i] := t;
		l := (l+1);
		r := (r+1);
		i := (i+1)
	else
		if t < w then
			l := (l+1)
		else
			r := (r+1)
		fi
	fi
od